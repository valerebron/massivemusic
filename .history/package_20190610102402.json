    "bootstrap": "^4.1.3",
    "cheerio": "^1.0.0-rc.2",
    "connect": "^3.6.6",
    "connect-history-api-fallback": "^1.6.0",
    "express": "^4.16.4",
    "moment": "^2.22.2",
    "mongoose": "^5.3.14",
    "nightmare": "^3.0.1",
    "socket.io": "^2.2.0",
    "socket.io-client": "^2.2.0",
    "uws": "^99.0.0",
    "vue": "^2.5.17",
    "vue-router": "^3.0.2",
    "vue-youtube": "^1.3.4"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.2.0",
    "@vue/cli-plugin-eslint": "^3.2.1",
